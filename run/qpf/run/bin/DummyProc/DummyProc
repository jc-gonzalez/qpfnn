    else
        date
        echo ""
        ls -lisan $i
        stat $i
        echo ""
        date
    fi
    sleep 4
done > $output

cat $output > $log

exit 0

##----------------------------------------------------------------------------

declare -a stt
k=0
for i in $files ; do
    b=$(basename $i)
    data="{\""$(stat -s $i |sed -e 's#st_##g' -e 's#=#":"#g' -e 's# #", "#g')"\"}"
    stt[$k]="\"$b\": $data"
    k=$((k + 1))
done

content=$(echo ${stt[@]} | sed -e 's#} "#}, "#g')

echo "{$content}" >> $output

ls -l $output

sleep 10

exit 0
